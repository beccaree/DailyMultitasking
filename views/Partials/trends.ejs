<!-- Morris Chart Dependencies -->
<script src="js/plugin/morris/raphael.min.js"></script>
<script src="js/plugin/morris/morris.min.js"></script>

<script type="text/javascript">
    // PAGE RELATED SCRIPTS

    /*
     * Run all morris chart on this page
     */
    $(document).ready(function () {

        pageSetUp();

        if ($('#trends-graph').length) {

            Morris.Area({
                element: 'trends-graph',
                data: [{
                    date: '2010 Q1',
                    numTasks: 0,
                }, {
                    date: '2010 Q2',
                    numTasks: 0,
                }, {
                    date: '2010 Q3',
                    numTasks: 0,
                }, {
                    date: '2010 Q4',
                    numTasks: 0,
                }, {
                    date: '2011 Q1',
                    numTasks: 0,
                }, {
                    date: '2011 Q2',
                    numTasks: 0,
                }, {
                    date: '2011 Q3',
                    numTasks: 0,
                }, {
                    date: '2011 Q4',
                    numTasks: 0,
                }, {
                    date: '2012 Q1',
                    numTasks: 0,
                }, {
                    date: '2012 Q2',
                    numTasks: 1,
                }],
                xkey: 'date',
                ykeys: ['numTasks'],
                labels: ["Tasks Complete"],
                pointSize: 2,
                hideHover: 'auto'
            });

        }
    })

</script>