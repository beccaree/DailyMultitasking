<div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
    <!-- Widget ID (each widget will need unique ID)-->
    <div class="jarviswidget" id="wid-id-0" data-widget-editbutton="false">
        <!-- widget options:
        usage: <div class="jarviswidget" id="wid-id-0" data-widget-editbutton="false">

        data-widget-colorbutton="false"
        data-widget-editbutton="false"
        data-widget-togglebutton="false"
        data-widget-deletebutton="false"
        data-widget-fullscreenbutton="false"
        data-widget-custombutton="false"
        data-widget-collapsed="true"
        data-widget-sortable="false"

        -->
        <header>
            <span class="widget-icon"> <i class="fa fa-bar-chart-o"></i> </span>
            <h2>Task Trends</h2>
        </header>

        <!-- widget div-->
        <div>

            <!-- widget edit box -->
            <div class="jarviswidget-editbox">
                <!-- This area used as dropdown edit box -->
                <input type="text">
            </div>
            <!-- end widget edit box -->

            <!-- widget content -->
            <div class="widget-body no-padding">

                <div id="trends-graph" class="chart no-padding"></div>

            </div>
            <!-- end widget content -->

        </div>
        <!-- end widget div -->

    </div>
    <!-- end widget -->
</div>

<div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">

    <div class="jarviswidget" id="wid-id-0" data-widget-editbutton="false">

        <header>
            <span class="widget-icon"> <i class="fa fa-pie-chart"></i> </span>
            <h2>Daily Progress</h2>
        </header>

        <div style="text-align: center;">
            <div class="easy-pie-chart txt-color-blue easyPieChart" data-percent="100" data-pie-size="80">
                <p>Sunday</p>
                <span class="percent percent-sign txt-color-blue font-md" style="margin-top: 0px;">100</span>
            </div>
            <div class="easy-pie-chart txt-color-pinkDark easyPieChart" data-percent="66" data-pie-size="100">
                <p>Monday</p>
                <span class="percent percent-sign txt-color-pinkDark font-md" style="margin-top: 0px;">66</span>
            </div>
            <div class="easy-pie-chart txt-color-blue easyPieChart" data-percent="50" data-pie-size="80">
                <p>Tuesday</p>
                <span class="percent percent-sign txt-color-blue font-md" style="margin-top: 0px;">50</span>
            </div>
            <div class="easy-pie-chart txt-color-blue easyPieChart" data-percent="75" data-pie-size="80">
                <p>Wednesday</p>
                <span class="percent percent-sign txt-color-blue font-md" style="margin-top: 0px;">75</span>
            </div>
            <div class="easy-pie-chart txt-color-blue easyPieChart" data-percent="0" data-pie-size="80">
                <p>Thursday</p>
                <span class="percent percent-sign txt-color-blue font-md" style="margin-top: 0px;">0</span>
            </div>
            <div class="easy-pie-chart txt-color-blue easyPieChart" data-percent="100" data-pie-size="80">
                <p>Friday</p>
                <span class="percent percent-sign txt-color-blue font-md" style="margin-top: 0px;">100</span>
            </div>
            <div class="easy-pie-chart txt-color-blue easyPieChart" data-percent="70" data-pie-size="80">
                <p>Saturday</p>
                <span class="percent percent-sign txt-color-blue font-md" style="margin-top: 0px;">70</span>
            </div>
        </div>

    </div>
</div>

<!-- EASY PIE CHARTS -->
<script src="js/plugin/easy-pie-chart/jquery.easy-pie-chart.min.js"></script>

<!-- Morris Chart Dependencies -->
<script src="js/plugin/morris/raphael.min.js"></script>
<script src="js/plugin/morris/morris.min.js"></script>

<script type="text/javascript">
    // PAGE RELATED SCRIPTS

    /*
     * Run all morris chart on this page
     */
    $(document).ready(function() {

        pageSetUp();

        if ($('#trends-graph').length) {

            Morris.Area({
                element : 'trends-graph',
                data : [{
                    date : '2010 Q1',
                    numTasks : 2,
                }, {
                    date : '2010 Q2',
                    numTasks : 2,
                }, {
                    date : '2010 Q3',
                    numTasks : 4,
                }, {
                    date : '2010 Q4',
                    numTasks : 3,
                }, {
                    date : '2011 Q1',
                    numTasks : 6,
                }, {
                    date : '2011 Q2',
                    numTasks : 5,
                }, {
                    date : '2011 Q3',
                    numTasks : 4,
                }, {
                    date : '2011 Q4',
                    numTasks : 15,
                }, {
                    date : '2012 Q1',
                    numTasks : 10,
                }, {
                    date : '2012 Q2',
                    numTasks : 8,
                }],
                xkey : 'date',
                ykeys : ['numTasks'],
                labels : ["Tasks Complete"],
                pointSize : 2,
                hideHover : 'auto'
            });

        }
    })
</script>
